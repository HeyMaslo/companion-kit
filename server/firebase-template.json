{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
      "cd \"$RESOURCE_DIR\" && yarn lint",
      "cd \"$RESOURCE_DIR\" && yarn build"
    ],
    "source": "functions"
  },
  "hosting": [
    {
      "target": "dashboard-staging",
      "public": "public/dashboard",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "dashboard-prod",
      "public": "public/dashboard",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "web-staging",
      "public": "public/web",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    {
      "target": "web-prod",
      "public": "public/web",
      "cleanUrls": true,
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  ],
  "storage": {
    "rules": "storage.rules"
  },
"emulators": {
    "ui": {
        "enabled": true,
        "host": "${FB_EMULATOR_UI_ADDR}",
        "port": "${FB_EMULATOR_UI_PORT}"
    },
    "firestore": {
        "port": 5555
    },
    "functions": {
        "port": 5001
    },
    "hosting": {
        "port": 5000
    },
    "auth": {
        "port": 9099
    }
  }
}