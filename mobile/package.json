{
  "name": "maslo-coaching-client",
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.3.5",
  "private": true,
  "scripts": {
    "start": "node setEnv development expo start",
    "start:prod": "node setEnv production expo start -c",
    "publish:stage": "node setEnv staging expo publish -c",
    "publish:prod": "node setEnv production expo publish -c",
    "build:ios:stage": "node setEnv staging expo build:ios",
    "build:ios:prod": "node setEnv production expo build:ios",
