    "build:android:stage:nopublish": "yarn build:android:stage --no-publish",
    "build:android:prod": "node setEnv production expo build:android -t app-bundle",
    "build:android:prod:nopublish": "yarn build:android:prod --no-publish",
    "check-ts": "ttsc --watch",
    "start:check-ts": "concurrently --handle-input \"yarn start\" \"yarn check-ts\""
  },
