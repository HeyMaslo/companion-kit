{
  "name": "CompanionKit",
  "displayName": "CompanionKit",
  "slug": "companionkit",
  "privacy": "unlisted",
  "platforms": [
    "ios",
    "android"
  ],
  "version": "1.0.1",
  "orientation": "portrait",
  "icon": "./src/assets/appicons/appicon.png",
  "splash": {
  "resizeMode": "contain",
    "backgroundColor": "#FCFCFC"
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": [
    "src/assets/fonts/app/*",
    "src/assets/images/*",
    "src/assets/images/locations/*",
    "src/assets/images/app/**"
  ],
  "isDetached": false,
  "extra": {
    "softVersion": "1"
  },
  "ios": {
    "supportsTablet": false,
    "usesAppleSignIn": true,
    "icon": "./src/assets/appicons/appicon.png",
    "buildNumber": "1",
    "bundleIdentifier": "com.maslo.companionkit",
    "config": {
      "usesNonExemptEncryption": false,
      "googleSignIn": {
        "reservedClientId": "..."
      }
    },
    "associatedDomains": [
      "..."
    ],
    "googleServicesFile": "./configs/app/GoogleService-Info.plist",
    "infoPlist": {
      "NSMicrophoneUsageDescription": "Enable this app to record your response to this check-in prompt.",
      "NSPhotoLibraryUsageDescription": "..."
    }
  },
  "android": {
    "config": {
      "googleSignIn": {
        "apiKey": "...",
        "certificateHash": "..."
      }
    },
    "package": "com.maslo.companionkit",
    "googleServicesFile": "./configs/app/google-services.json",
    "versionCode": 1,
    "intentFilters": [
      {
        "action": "VIEW",
        "autoVerify": true,
        "data": [
          {
            "scheme": "https",
            "host": "..."
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ]
  },
  "hooks": {
    "postPublish": [
      {
        "file": "sentry-expo/upload-sourcemaps",
        "config": {
          "organization": "...",
          "project": "...",
          "authToken": "..."
        }
      }
    ]
  },
  "packagerOpts": {
    "config": "...",
    "sourceExts": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "svg"
    ],
    "assetExts": [
      "md",
      "glsl"
    ]
  }
}